    "async-lock": "^1.1.3",
    "botkit": "^0.6.21",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "downloadjs": "^1.4.7",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "googleapis": "^27.0.0",
    "nuxt": "^2.3.4",
    "parse-srt": "^1.0.0-alpha",
    "pg": "^7.8.0",
